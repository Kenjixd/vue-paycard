    "credit-card",
    "ui",
    "vite",
    "component"
  ],
  "license": "MIT",
  "repository": "github:Kenjixd/vue-paycard",
  "packageManager": "yarn@4.9.1",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5",
    "vite": "^5.0.0",
    "vue": "^3.4.0"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "prepublishOnly": "yarn build"
